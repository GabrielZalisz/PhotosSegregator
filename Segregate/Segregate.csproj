<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0-windows</TargetFramework>
    <OutputType>WinExe</OutputType>
    <AssemblyName>TridicFotek</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <UseWPF>true</UseWPF>
    <ImportWindowsDesktopTargets>true</ImportWindowsDesktopTargets>
    <Platforms>AnyCPU</Platforms>
	<SatelliteResourceLanguages>en,cs</SatelliteResourceLanguages>
  </PropertyGroup>
  <PropertyGroup>
    <ApplicationIcon>Sites 3.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <AppDesigner Include="Properties\" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Images\navod.jpg" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Images\Výstřižek.JPG" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Images\Sites 3.ico" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Sites 3.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="ExifPhotoReader" Version="1.0.4" />
  </ItemGroup>
</Project>